{
    "name": "vespolina/store-bundle",
    "type": "symfony-bundle",
    "description": "Vespolina Store bundle. Store management and more.",
    "keywords": ["shop", "ecommerce", "store", "item"],
    "homepage": "http://vespolina-project.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Vespolina Team",
            "homepage": "https://github.com/vespolina/VespolinaStoreBundle/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/symfony": "2.1.*",
        "vespolina/order-bundle": "dev-master",
        "vespolina/partner-bundle": "dev-master",
        "vespolina/product-bundle": "dev-master",
        "vespolina/taxation-bundle": "dev-master"
    },
    "suggest": {
        "symfony/doctrine-bundle": "*",
        "symfony/mongodb-odm-bundle": "*"
    },
    "autoload": {
        "psr-0": { "Vespolina\\StoreBundle": "" }
    },
    "target-dir": "Vespolina/StoreBundle",
    "minimum-stability": "dev"
}

