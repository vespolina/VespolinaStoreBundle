{
    "name": "vespolina/store-bundle",
    "type": "symfony-bundle",
    "description": "Vespolina Store bundle. Store management and more.",
    "keywords": ["vespolina", "shop", "ecommerce", "store"],
    "homepage": "http://vespolina-project.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Vespolina Team",
            "homepage": "https://github.com/vespolina/VespolinaStoreBundle/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/symfony": ">=2.1",
        "vespolina/core": "*",
        "vespolina/taxation-bundle": "*"
    },
    "autoload": {
        "psr-0": { "Vespolina\\StoreBundle": "" }
    },
    "target-dir": "Vespolina/StoreBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}

